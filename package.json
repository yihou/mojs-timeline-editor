{
  "name": "@mojs/timeline-editor",
  "version": "0.19.2",
  "description": "mojs GUI for editing timeline/html elements",
  "keywords": [
    "mojs",
    "motion",
    "effects",
    "animation",
    "motion",
    "motion graphics",
    "timeline editor",
    "svg"
  ],
  "main": "dist/timeline-editor.js",
  "umd:main": "dist/timeline-editor.umd.js",
  "module": "dist/timeline-editor.modern.js",
  "exports": {
    "require": "./dist/timeline-editor.js",
    "default": "./dist/timeline-editor.modern.js"
  },
  "types": "dist/MojsTimelineEditor.d.ts",
  "source": "src/MojsTimelineEditor.tsx",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "microbundle-crl --name @mojs/timeline-editor --jsx React.createElement --no-compress",
    "start": "microbundle-crl watch --name @mojs/timeline-editor --jsx React.createElement --no-sourcemap --no-compress",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "typesync": "npx typesync",
    "type-check": "tsc --noEmit --p ./tsconfig.json",
    "dedup": "npx yarn-deduplicate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/legomushroom/mojs-timeline-editor.git"
  },
  "author": {
    "name": "Oleg Solomka",
    "email": "legomushroom@gmail.com",
    "twitter": "@legomushroom",
    "url": "https://github.com/legomushroom/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/legomushroom/mojs-timeline-editor/issues"
  },
  "homepage": "https://github.com/legomushroom/mojs-timeline-editor#readme",
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "@egjs/hammerjs": "^2.0.17",
    "@mojs/core": "^1.1.0",
    "@mojs/curve-editor": "^1.7.1",
    "@mojs/player": "^1.0.1",
    "@reduxjs/toolkit": "^1.6.1",
    "decko": "^1.2.0",
    "hammerjs": "^2.0.8",
    "lint-staged": "^11.1.2",
    "md5": "^2.3.0",
    "propagating-hammerjs": "^2.0.1",
    "react": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-recycle": "^1.4.0",
    "redux-undo": "^1.0.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/babel__core": "^7.1.15",
    "@types/eslint": "^7.28.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/gh-pages": "^3.2.0",
    "@types/hammerjs": "^2.0.40",
    "@types/jest": "^27.0.1",
    "@types/md5": "^2.3.1",
    "@types/node": "^14.17.9",
    "@types/prettier": "^2.3.2",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@types/redux-recycle": "^1.2.3",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/testing-library__user-event": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^3.2.3",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "typescript": "^4.3.5"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "yarn run eslint"
    ]
  }
}
